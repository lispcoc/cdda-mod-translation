msgstr "洞察強化[13]"

msgid "Insight [14]"
msgstr "洞察強化[14]"

msgid "Insight [15+]"
msgstr "洞察強化[15+]"

msgid "Insight [2]"
msgstr "洞察強化[2]"

msgid "Insight [3]"
msgstr "洞察強化[3]"

msgid "Insight [4]"
msgstr "洞察強化[4]"

